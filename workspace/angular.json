{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "una-docs": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/una-docs",
      "sourceRoot": "projects/una-docs/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/una-docs",
            "index": "projects/una-docs/src/index.html",
            "main": "projects/una-docs/src/main.ts",
            "polyfills": "projects/una-docs/src/polyfills.ts",
            "tsConfig": "projects/una-docs/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/una-docs/src/favicon.ico",
              "projects/una-docs/src/assets"
            ],
            "styles": [
              "projects/una-docs/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/una-docs/src/environments/environment.ts",
                  "with": "projects/una-docs/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "port": 4200
          },
          "configurations": {
            "production": {
              "browserTarget": "una-docs:build:production"
            },
            "development": {
              "browserTarget": "una-docs:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "una-docs:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/una-docs/src/test.ts",
            "polyfills": "projects/una-docs/src/polyfills.ts",
            "tsConfig": "projects/una-docs/tsconfig.spec.json",
            "karmaConfig": "projects/una-docs/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/una-docs/src/favicon.ico",
              "projects/una-docs/src/assets"
            ],
            "styles": [
              "projects/una-docs/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "una-theme-kit-demo-app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/una-theme-kit-demo-app",
      "sourceRoot": "projects/una-theme-kit-demo-app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/una-theme-kit-demo-app",
            "index": "projects/una-theme-kit-demo-app/src/index.html",
            "browser": "projects/una-theme-kit-demo-app/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/una-theme-kit-demo-app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/una-theme-kit-demo-app/public"
              }
            ],
            "styles": [
              "projects/una-theme-kit-demo-app/src/styles.scss"
            ],
            "scripts": [],
            "server": "projects/una-theme-kit-demo-app/src/main.server.ts",
            "prerender": true,
            "ssr": {
              "entry": "projects/una-theme-kit-demo-app/server.ts"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kB",
                  "maximumError": "4kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "una-theme-kit-demo-app:build:production"
            },
            "development": {
              "buildTarget": "una-theme-kit-demo-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/una-theme-kit-demo-app/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/una-theme-kit-demo-app/public"
              }
            ],
            "styles": [
              "projects/una-theme-kit-demo-app/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
